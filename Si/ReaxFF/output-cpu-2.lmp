  1 by 1 by 1 MPI processor grid
create_atoms    1 region myreg
Created 64 atoms
  using lattice units in orthogonal box = (0 0 0) to (10.98 10.98 10.98)
  create_atoms CPU = 0.000 seconds
variable 	mass equal 28.0855
mass            1 ${mass}
mass            1 28.0855

# Other setup
timestep	0.001
neigh_modify    delay 10 every 1
pair_style      reaxff NULL mincap 5000
pair_coeff      * * /hpcgpfs01/work/csi/hvandam/DeePMD/GPU/lammps/bench/POTENTIALS/ffield.reax Si
WARNING: Changed valency_val to valency_boc for X (../reaxff_ffield.cpp:289)
thermo          ${out_freq}
thermo          100
thermo_style 	custom step temp pe etotal epair emol press lx ly lz vol
restart 	${out_freq} restart.lmp restart2.lmp
restart 	100 restart.lmp restart2.lmp

fix             1 all property/atom q ghost yes
fix             2 all qeq/reaxff 1 0.0 10.0 1.0e-6 reaxff

# Equilibrate in NPT

fix             3 all nph iso ${pressure} ${pressure} ${pressureDamp}
fix             3 all nph iso 1 ${pressure} ${pressureDamp}
fix             3 all nph iso 1 1 ${pressureDamp}
fix             3 all nph iso 1 1 1
fix             4 all temp/csvr ${temperature} ${temperature} ${tempDamp} ${seed}
fix             4 all temp/csvr 300 ${temperature} ${tempDamp} ${seed}
fix             4 all temp/csvr 300 300 ${tempDamp} ${seed}
fix             4 all temp/csvr 300 300 0.1 ${seed}
fix             4 all temp/csvr 300 300 0.1 745823
fix             5 all momentum 100 linear 1 1 1 rescale

velocity 	all create ${temperature} ${seed} dist gaussian
velocity 	all create 300 ${seed} dist gaussian
velocity 	all create 300 745823 dist gaussian

run             1000 upto # 1 ps

